node ck2_clk (c: bool; d: bool when c; e: int when d)
returns (n: int);
var a: bool;
let
  a = c and current(d);
  n = if a then 0 else current(current(e));
tel

node ck2 (c: bool; d: bool; e: int)
returns (n: int);
var
  d_on_c : bool when c;

let
  d_on_c = d when c;
  n = ck2_clk(c,  d_on_c , e when d_on_c);
tel
